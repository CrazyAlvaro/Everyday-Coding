{
  "name": "ant-blade",
  "version": "0.2.11",
  "description": "ant blade project",
  "main": "server-compiled/index.js",
  "engine": {
    "node": ">=5.11.1"
  },
  "scripts": {
    "commit": "git-cz",
    "run:prod": "cross-env NODE_ENV=production node server-compiled",
    "db:installDefaults": "babel-node ./installs",
    "db:migrate": "cd db-migrate && db-migrate up",
    "clean": "rimraf dist server-compiled",
    "prebuild": "npm run clean && npm run lint",
    "build:server": "cross-env NODE_ENV=production babel server -d server-compiled",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run build:webpack && npm run build:server",
    "start": "babel-node server",
    "precoverage": "npm run lint",
    "start:dev": "nodemon --exec babel-node --presets=es2015 server",
    "coverage": "nyc --extension .jsx mocha",
    "lint": "eslint src server test",
    "pretest": "npm run lint",
    "test": "mocha test",
    "test:watch": "npm run test -- --watch",
    "test:karma": "NODE_ENV=test karma start karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Madadata/ant-blade.git"
  },
  "keywords": [
    "webpack",
    "starter"
  ],
